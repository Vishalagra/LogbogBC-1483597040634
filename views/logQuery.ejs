<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="css/styles.css">
<link rel="stylesheet"
	href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
<script
	src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
<script
	src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
<script
	src="http://ajax.googleapis.com/ajax/libs/angularjs/1.3.14/angular.min.js"></script>
<script src="js/controller.js" type="text/javascript"></script>

<title>LogBook Query</title>
</head>
<body ng-app="queryModal" class=loginBody>
	<nav class="navbar navbar-inverse navbar-static-top">
		<div class="container">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle collapsed"
					data-toggle="collapse" data-target="#bs-example-navbar-collapse-1"
					aria-expanded="false">
					<span class="sr-only">Toggle navigation</span> <span
						class="icon-bar"></span> <span class="icon-bar"></span> <span
						class="icon-bar"></span>
				</button>
				<a class="navbar-brand" href="#">LogBog App</a>
			</div>
			<div class="collapse navbar-collapse"
				id="bs-example-navbar-collapse-1">
				<ul class="nav navbar-nav navbar-left">
					<li><a href="/"> Home -> </a></li>
					<li><a href="/login"> LogEntry -> </a></li>
					<li><a href="/logEntry"> LogQuery</a></li>
				</ul>
				<ul class="nav navbar-nav navbar-right">
					<li><a href="#"><span class="glyphicon glyphicon-user"></span>User</a></li>
					<li><a href="/"><span class="glyphicon glyphicon-log-in"></span>
							Logout</a></li>
				</ul>
			</div>
		</div>
	</nav>
	<form name="logQueryForm" role="form" action="/logQuery" method="post">
		<div ng-controller="QueryCtrl" class="container">
			<div>
				<div>
					<h4>
						User Name: <%= user %> <br> Senr: <%= senr %>
					</h4>
				</div>
				<table>
					<tr colspan="3">
						<!-- <td> fv188 ada - genstartebj works. </td> -->
					</tr>
					<tr>
						<td>Senr :</td>
						<td><input type="text" class="form-control" id="senr"
							ng-model="senr" autofocus="" maxlength="9" minlength="9" required />
						</td>
						<td><input type="button" class="btn btn-primary"
							name="search" value="Search" onclick="show()" /></td>

					</tr>
				</table>

			</div>
			<hr>

			<table class="table table-striped table-bordered dataQueryTable"
				id="queryData">

				<tr>
					<th align="center">CPR Nummer</th>
					<th align="center">CPR Navn</th>
					<th align="center">Date</th>
					<th align="center">Duration</th>
				</tr>

				<tr ng-repeat="item in data">
					<td><input type="text" class="form-control"
						value="{{item.CPRNum}}" disabled="disabled" /></td>
					<td><input type="text" class="form-control"
						value="{{ item.CPRNavn }}" disabled="disabled" /></td>
					<td><input type="text" class="form-control"
						value="{{ item.DOW }}" disabled="disabled" /></td>
					<td><input type="text" class="form-control"
						value="{{ item.NoOfHours }}" disabled="disabled" /></td>
				</tr>
			</table>
			<!--
 <div class ="button">
 		<button type="submit" ng-click="toggleOK()" class="btn btn-default pull-center">Submit Comments</button>
     <!--  <button type="submit" class="btn btn-default" name = "submitData">submit comments</button> 
 </div> -->
 	</div>
	</form>

	<script>
		function show() {
			if (document.getElementById("senr").value != '') {
				document.getElementById("queryData").style.display = "table";
			}
		}
	</script>

</body>
</html>